{
  "name": "gaia-admin",
  "version": "0.0.1",
  "description": "Service for administrating a Gaia hub",
  "main": "lib/index.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "start-dev": "ts-node src/index.ts",
    "build": "tsc && chmod +x lib/index.js",
    "lint": "eslint --ext .ts ./src",
    "test": "npm run lint && NODE_ENV=test nyc node ./test/test.ts",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "bin": {
    "blockstack-gaia-admin": "lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockstack/gaia"
  },
  "keywords": [
    "decentralized",
    "storage"
  ],
  "author": "Jude Nelson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blockstack/gaia"
  },
  "dependencies": {
    "ajv": "^6.9.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.1",
    "express": "^4.16.4",
    "express-winston": "^3.1.0",
    "winston": "^3.2.1",
    "snyk": "^1.290.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/register": "^7.0.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/express-winston": "^3.0.1",
    "@types/node": "^10.14.10",
    "@types/tape": "^4.2.33",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-istanbul": "^5.1.4",
    "eslint": "^6.0.1",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.12",
    "tape": "^4.10.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "homepage": "https://github.com/blockstack/gaia",
  "nyc": {
    "cache": false,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "snyk": true
}
